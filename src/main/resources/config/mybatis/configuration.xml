<?xml version="1.0" encoding="utf8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
    <settings>
        <setting name="cacheEnabled" value="true"/>
    </settings>
    <typeAliases>
        <typeAlias alias="IdentityCode" type="com.fmc.edu.model.common.IdentityCode"/>
        <typeAlias alias="BaseProfile" type="com.fmc.edu.model.profile.BaseProfile"/>
        <typeAlias alias="ParentProfile" type="com.fmc.edu.model.profile.ParentProfile"/>
        <typeAlias alias="ParentStudentRelationship" type="com.fmc.edu.model.relationship.ParentStudentRelationship"/>
        <typeAlias alias="Address" type="com.fmc.edu.model.address.Address"/>
        <typeAlias alias="TeacherProfile" type="com.fmc.edu.model.profile.TeacherProfile"/>
        <typeAlias alias="FmcClass" type="com.fmc.edu.model.school.FmcClass"/>
        <typeAlias alias="School" type="com.fmc.edu.model.school.School"/>
        <typeAlias alias="Student" type="com.fmc.edu.model.student.Student"/>
        <typeAlias alias="Permission" type="com.fmc.edu.model.autho.Permission"/>
        <typeAlias alias="Role" type="com.fmc.edu.model.autho.Role"/>
        <typeAlias alias="RolePermission" type="com.fmc.edu.model.autho.RolePermission"/>
        <typeAlias alias="UserRole" type="com.fmc.edu.model.autho.UserRole"/>
    </typeAliases>
    <typeHandlers>
        <typeHandler handler="org.apache.ibatis.type.BlobTypeHandler" javaType="byte[]" jdbcType="BLOB"/>
    </typeHandlers>
</configuration>